{
    "name": "alexpts/php-transports",
    "description": "Any transports",
    "homepage": "https://github.com/alexpts/php-transports",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "udp",
        "tcp",
        "unix",
        "socket",
        "transport",
        "net"
    ],
    "config": {
        "optimize-autoloader": true
    },
    "authors": [
        {
            "name": "Alexpts",
            "email": "alexpts3@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "~7.2",
        "ext-mbstring": "*",
        "ext-sockets": "*"
    },
    "autoload": {
        "psr-4": {
            "PTS\\Transport\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1"
    }
}
